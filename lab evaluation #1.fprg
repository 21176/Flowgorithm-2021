    </attributes>
    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="m" type="Real" array="False" size=""/>
            <declare name="p" type="Real" array="False" size=""/>
            <declare name="c" type="Real" array="False" size=""/>
            <declare name="totalmarks" type="Real" array="False" size=""/>
            <declare name="totalmp" type="Real" array="False" size=""/>
            <output expression="&quot;Enter the marks obtained in maths: &quot;" newline="True"/>
            <input variable="m"/>
            <output expression="&quot;Enter the marks obtained in physics: &quot;" newline="True"/>
            <input variable="p"/>
            <output expression="&quot;Enter the marks obtained in chemistry: &quot;" newline="True"/>
            <input variable="c"/>
            <assign variable="totalmarks" expression="m+p+c"/>
            <assign variable="totalmp" expression="m+p"/>
            <output expression="&quot;The total marks obtained by the candidate in all three subjects: &quot; &amp;totalmarks" newline="True"/>
            <output expression="&quot;The marks obtained by the candidate in both maths and physics: &quot; &amp;totalmp" newline="True"/>
            <if expression="m&gt;=65&amp;&amp;p&gt;=55&amp;&amp;c&gt;=50&amp;&amp;totalmarks&gt;=190||m&gt;=65&amp;&amp;p&gt;=55&amp;&amp;c&gt;=50&amp;&amp;totalmp&gt;=140">
                <then>
                    <output expression="&quot;The candidate is eligible &quot;" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;The candidate is not eligible &quot;" newline="True"/>
                </else>
            </if>
        </body>
    </function>
