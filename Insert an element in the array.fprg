    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="i, N, ele, pos" type="Integer" array="False" size=""/>
            <output expression="&quot;Enter how many elements? &quot;" newline="True"/>
            <input variable="N"/>
            <declare name="num" type="Integer" array="True" size="N"/>
            <output expression="&quot;Enter the elements for the array: &quot;" newline="True"/>
            <for variable="i" start="0" end="N-1" direction="inc" step="1">
                <input variable="num[i]"/>
            </for>
            <output expression="&quot;Enter the element to replace: &quot;" newline="True"/>
            <input variable="ele"/>
            <output expression="&quot;Enter the position: &quot;" newline="True"/>
            <input variable="pos"/>
            <assign variable="num[pos-1]" expression="ele"/>
            <for variable="i" start="0" end="N-1" direction="inc" step="1">
                <output expression="num[i]" newline="True"/>
            </for>
        </body>
