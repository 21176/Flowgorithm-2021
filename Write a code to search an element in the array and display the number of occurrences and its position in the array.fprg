    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="i, N, ele, count" type="Integer" array="False" size=""/>
            <assign variable="count" expression="0"/>
            <output expression="&quot;Enter the size of the array: &quot;" newline="True"/>
            <input variable="N"/>
            <declare name="a" type="Integer" array="True" size="N"/>
            <output expression="&quot;Enter the elements of the array: &quot;" newline="True"/>
            <for variable="i" start="0" end="N-1" direction="inc" step="1">
                <input variable="a[i]"/>
            </for>
            <output expression="&quot;Enter the search element: &quot;" newline="True"/>
            <input variable="ele"/>
            <for variable="i" start="0" end="N-1" direction="inc" step="1">
                <if expression="a[i]=ele">
                    <then>
                        <assign variable="count" expression="count+1"/>
                        <output expression="&quot;position: &quot; &amp;i" newline="True"/>
                    </then>
                    <else/>
                </if>
            </for>
            <output expression="&quot;occurance: &quot;&amp;count" newline="True"/>
        </body>
    </function>
