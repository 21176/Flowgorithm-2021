    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="i, sum, N, avg" type="Real" array="False" size=""/>
            <output expression="&quot;Enter size of the array: &quot;" newline="True"/>
            <input variable="N"/>
            <declare name="num" type="Integer" array="True" size="N"/>
            <output expression="&quot;Enter the elements for the array: &quot;" newline="True"/>
            <assign variable="sum" expression="0"/>
            <for variable="i" start="0" end="N-1" direction="inc" step="1">
                <input variable="num[i]"/>
                <assign variable="sum" expression="sum+num[i]"/>
            </for>
            <output expression="&quot;The sum of elements in the array is: &quot;&amp;sum" newline="True"/>
            <assign variable="avg" expression="sum/N"/>
            <output expression="&quot;The average of elements in the array is: &quot;&amp;avg" newline="True"/>
        </body>
    </function>
