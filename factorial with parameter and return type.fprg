    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="num, c" type="Integer" array="False" size=""/>
            <output expression="&quot;Enter a number: &quot;" newline="True"/>
            <input variable="num"/>
            <call expression="fact(num)"/>
            <assign variable="c" expression="fact(num)"/>
            <output expression="&quot;The factorial of the number is: &quot;&amp;c" newline="True"/>
        </body>
    </function>
    <function name="fact" type="Integer" variable="fac">
        <parameters>
            <parameter name="num" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="i, fac" type="Integer" array="False" size=""/>
            <assign variable="fac" expression="1"/>
            <for variable="i" start="1" end="num" direction="inc" step="1">
                <assign variable="fac" expression="fac*i"/>
            </for>
        </body>
    </function>
